<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
	<display-name></display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>

	<listener>
		<listener-class>initializationAndShutdown.DataInitialization</listener-class>
	</listener>
	<servlet>
		<servlet-name>selectTicketNum</servlet-name>
		<servlet-class>controller.SelectTicketNum</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>movieInformation</servlet-name>
		<servlet-class>controller.MovieInformation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>isCludeUserName</servlet-name>
		<servlet-class>controller.IsCludeUserName</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>isCludeUserEmail</servlet-name>
		<servlet-class>controller.IsCludeUserEmail</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>registerController</servlet-name>
		<servlet-class>controller.RegisterController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>controller.LoginController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>isLogined</servlet-name>
		<servlet-class>controller.IsLogined</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>rushTicketController</servlet-name>
		<servlet-class>controller.RushTicketController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>quitController</servlet-name>
		<servlet-class>controller.QuitController</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>selectTicketNum</servlet-name>
		<url-pattern>/selectTicketNum.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>movieInformation</servlet-name>
		<url-pattern>/movieInformation.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>isCludeUserName</servlet-name>
		<url-pattern>/isCludeUserName.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>isCludeUserEmail</servlet-name>
		<url-pattern>/isCludeUserEmail.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>registerController</servlet-name>
		<url-pattern>/registerController.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>isLogined</servlet-name>
		<url-pattern>/isLogined.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>rushTicketController</servlet-name>
		<url-pattern>/rushTicketController.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>quitController</servlet-name>
		<url-pattern>/quitController.do</url-pattern>
	</servlet-mapping>
</web-app>
